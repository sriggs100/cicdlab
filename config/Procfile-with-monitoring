restproxy: java -Dcom.sun.management.jmxremote=true -Dcom.sun.management.jmxremote.local.only=true -Dcom.sun.management.jmxremote.port=49000 -Dcom.sun.management.jmxremote.rmi.port=49000 -Dcom.sun.management.jmxremote.authenticate=false -Dcom.sun.management.jmxremote.ssl=false -Djava.rmi.server.hostname=localhost -jar build/acqrestproxy-*.jar 
worker: chmod +x ./build/scripts/worker_proc.sh;./build/scripts/worker_proc.sh
csproxy: java -jar build/acqcsproxy-*.jar --spring.config.location=./build/config/acqcsproxy.properties